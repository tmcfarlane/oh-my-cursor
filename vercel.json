{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "",
  "framework": null,
  "rewrites": [
    { "source": "/install", "destination": "/install.sh" }
  ],
  "headers": [
    {
      "source": "/install",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Cache-Control", "value": "no-cache" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    },
    {
      "source": "/install.sh",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Cache-Control", "value": "no-cache" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}

